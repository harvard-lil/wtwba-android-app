package edu.harvad.law.librarylab.wtwba;

public class Item {
 
    //private variables
    String barcode;
    String user;
 
    // Empty constructor
    public Item(){
 
    }
    // constructor
    public Item(String incoming_barcode, String incoming_user){
        this.barcode = incoming_barcode;
        this.user = incoming_user;
    }

    // getting ID
    public String get_barcode(){
        return this.barcode;
    }
 
    // setting id
    public void set_barcode(string incming_barcode){
        this.barcode = incoming_barcode;
    }
 
    // getting name
    public String getName(){
        return this._name;
    }
 
    // setting name
    public void setName(String name){
        this._name = name;
    }
 
    // getting phone number
    public String getPhoneNumber(){
        return this._phone_number;
    }
 
    // setting phone number
    public void setPhoneNumber(String phone_number){
        this._phone_number = phone_number;
    }
}