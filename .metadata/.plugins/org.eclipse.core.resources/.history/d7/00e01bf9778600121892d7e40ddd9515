package edu.harvad.law.librarylab.wtwba;

public class Item {
 
    //private variables
    String barcode;
    String user;
 
    // Empty constructor
    public Item(){
 
    }
    // constructor
    public Item(String incoming_barcode, String incoming_user){
        this.barcode = incoming_barcode;
        this.user = incoming_user;
    }

    // getting ID
    public String get_barcode(){
        return this.barcode;
    }
 
    // setting id
    public void set_barcode(String incoming_barcode){
        this.barcode = incoming_barcode;
    }
 
    // getting name
    public String get_ser(){
        return this.user;
    }
 
    // setting name
    public void set_user(String incoming_user){
        this.user = incoming_user;
    }
}