package edu.harvad.law.librarylab.wtwba;

public class Item {
 
    //private variables
    String barcode;
    String title;
    // other attributes here
 
    // Empty constructor
    public Item(){
 
    }
    // constructor
    public Item(String incoming_barcode, String incoming_title){
        this.barcode = incoming_barcode;
        this.barcode = incoming_title;
    }

    // getting barcode
    public String get_barcode(){
        return this.barcode;
    }
 
    // setting barcode
    public void set_barcode(String incoming_barcode){
        this.barcode = incoming_barcode;
    }
 

}