package edu.harvad.law.librarylab.wtwba;

public class Item {
 
    //private variables
	int id;
    String barcode;
    String title;
    // other attributes here
 
    // Empty constructor
    public Item(){
 
    }
    // constructor
    public Item(String barcode, String title){
        this.barcode = barcode;
        this.barcode = title;
    }
    // constructor
    public Entry(int incoming_id, String incoming_barcode, String incoming_location){
    	this.id = incoming_id;
        this.barcode = incoming_barcode;
        this.location = incoming_location;
    }

	// getting barcode
    public String get_barcode(){
        return this.barcode;
    }
 
    // setting barcode
    public void set_barcode(String barcode){
        this.barcode = barcode;
    }
    
    public String get_title() {
		return title;
	}
	public void set_title(String title) {
		this.title = title;
	}
 

}